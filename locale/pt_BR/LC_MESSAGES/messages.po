#: conan_server_status.py:71
msgid "The server is operating normally."
msgstr "O servidor está operando normalmente."

#: conan_server_status.py:77
msgid "Online Players ({count})"
msgstr "Jogadores Online ({count})"

#: conan_server_status.py:79
msgid "Online Players (0)"
msgstr "Jogadores Online (0)"

#: conan_server_status.py:79
msgid "No one is playing at the moment."
msgstr "Ninguém está jogando no momento."

#: conan_server_status.py:81
msgid "Status updated"
msgstr "Status atualizado"

#: conan_server_status.py:86
msgid "RCON connection attempt {attempt}/{max_retries} failed: {error}"
msgstr "Tentativa de conexão RCON {attempt}/{max_retries} falhou: {error}"

#: conan_server_status.py:93
msgid "All RCON connection attempts have failed."
msgstr "Todas as tentativas de conexão RCON falharam."

#: conan_server_status.py:96
msgid "Could not connect to the server. It may be offline or restarting."
msgstr "Não foi possível conectar ao servidor. Pode estar desligado ou reiniciando."

#: conan_server_status.py:99
msgid "Check the console or contact an administrator."
msgstr "Verifique o console ou contate um administrador."

#: conan_server_status.py:112
msgid "Channel with ID {channel_id} for '{server_name}' not found."
msgstr "Canal com ID {channel_id} para '{server_name}' não encontrado."

#: conan_server_status.py:124
msgid "New status message created for '{server_name}' in channel {channel_name}."
msgstr "Nova mensagem de status criada para '{server_name}' no canal {channel_name}."

#: conan_server_status.py:126
msgid "Message for '{server_name}' not found. Creating a new one."
msgstr "Mensagem para '{server_name}' não encontrada. Criando uma nova."

#: conan_server_status.py:130
msgid "Error updating status for '{server_name}': {error}"
msgstr "Erro ao atualizar status para '{server_name}': {error}"

#: conan_server_status.py:136
msgid "Bot connected as {username}"
msgstr "Bot conectado como {username}"

#: conan_server_status.py:144
msgid "Found message for '{server_name}' in channel {channel_name}."
msgstr "Mensagem encontrada para '{server_name}' no canal {channel_name}."

#: conan_server_status.py:148
msgid "Status update task started."
msgstr "Tarefa de atualização de status iniciada."

#: conan_server_status.py:162
msgid "Command on cooldown. Try again in {seconds:.1f} seconds."
msgstr "Comando em tempo de recarga. Tente novamente em {seconds:.1f} segundos."

#: conan_server_status.py:171
msgid "Fatal error starting the bot: {error}"
msgstr "Erro fatal ao iniciar o bot: {error}"